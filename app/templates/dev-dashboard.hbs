<p>
    Hello {{model.lastName}}
</p>
{{#if editDevDetails}}
<form class="col s6">
    <div class="row">
        <div class="input-field col s3">
            {{input id="first_name" type="text" class="validate" value=firstName}}
            <label for="first_name">First Name</label>
        </div>
        <div class="input-field col s3">
            {{input id="last_name" type="text" class="validate" value=lastName}}
            <label for="last_name">Last Name</label>
        </div>
    </div>
    <div class="row">
        <div class="input-field col s3">
            {{input id="email" type="email" class="validate" value=email}}
            <label for="email">Email</label>
        </div>

        <div class="input-field col s3">
            {{input id="github" type="text" class="validate" value=github}}
            <label for="github_name">GitHub User Name</label>
        </div>
        <div class="input-field col s3">
            {{input id="password" type="password" class="validate" value=password}}
            <label for="github_name">password</label>
        </div>
    </div>
    <button class="btn waves-effect waves-light" type="submit" {{action 'saveChanges'}} name="action">Submit <i class="material-icons right">send</i></button>
</form>
{{else}}

<div class="container">
    <div class="fixed-action-btn">
        <a class="btn-floating btn-large blue" href="#modal1" {{action 'showDeveloperForm'}}><i class="large material-icons">mode_edit</i></a>

    </div>
</div>
{{/if}}